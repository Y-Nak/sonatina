# check: block1:
# nextln:   v2.i8 = add v1 v1;
# nextln:   return v2;
# check:  block2:
# nextln:   v3.i8 = add v1 v1;
# nextln:   return v3;
func %not_dominated_by_leader(v0.i1, v1.i8) -> i8:
    block0:
        br v0 block1 block2;

    block1:
        v2.i8 = add v1 v1;
        return v2;

    block2:
        v3.i8 = add v1 v1;
        return v3;
