target = "evm-ethereum-london"

# -(-v0) => v0
# check: return v0;
func public %neg0(v0.i8) -> i8 {
    block0:
        v1.i8 = neg v0;
        v2.i8 = neg v1;
        return v2;
}

# -(!v0) => v0
# check: v2.i16 = add v0 1.i16;
func public %neg1(v0.i16) -> i16 {
    block0:
        v1.i8 = not v0;
        v2.i8 = neg v1;
        return v2;
}
