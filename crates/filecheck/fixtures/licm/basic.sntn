func public %basic(v0.i32, v1.i32) -> void:
    block0:
        fallthrough block1;

    block1:
        v2.i32 = phi (0.i32 block0) (v4 block2);
        v3.i32 = add v0 v1;
        jump block2;

    block2:
        v4.i32 = add v2 v3;
        v5.i1 = slt v2 10.i32;
        br v5 block1 block3;

    block3:
        return;
