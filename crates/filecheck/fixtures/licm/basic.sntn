target = "evm-ethereum-london"

# check:  block0:
# nextln:      v3.i32 = add v0 v1;
# nextln:      fallthrough block1;
# nextln: 
# nextln:  block1:
# nextln:      v2.i32 = phi (0.i32 block0) (v4 block2);
# nextln:      jump block2;
# nextln: 
# nextln:  block2:
# nextln:      v4.i32 = add v2 v3;
# nextln:      v5.i1 = slt v2 10.i32;
# nextln:      br v5 block1 block3;
# nextln: 
