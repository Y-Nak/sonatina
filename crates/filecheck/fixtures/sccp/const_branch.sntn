#! `br` instruction with constant condition.

# sameln: func %const_branch() -> i8:
# nextln:     block0:
# nextln:         jump block2;
# nextln: 
# nextln:     block2:
# nextln:         jump block3;
# nextln: 
# nextln:     block3:
# nextln:         return 12.i8;
func %const_branch() -> i8:
    block0:
        v0.i1 = sgt -1.i8 0.i8;
        br v0 block1 block2;

    block1:
        jump block3;

    block2:
        jump block3;

    block3:
        return 12.i8;
