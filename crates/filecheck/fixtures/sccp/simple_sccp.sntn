#! Simple constant folding without branching.

# sameln: func %simple_fold():
# nextln:     block0:
# nextln:         v0.i8 = imm_u8 10;
# nextln:         v1.i8 = imm_u8 20;
# nextln:         v2.i8 = imm_u8 30;
# nextln:         return;
func %simple_fold():
    block0:
        v0.i8 = imm_i8 10;
        v1.i8 = imm_i8 20;
        v2.i8 = add v0 v1;
        return;
